@model HrBudget
@{
}
 <table class="table table-form table-dialog-form">

     <tr>
         <th class="w-100px">
             <label class="control-label">任务标题：</label>
         </th>
         <td>
             <p class="form-control-static">@Model.Title</p>
         </td>
         <th>
             <label class="control-label">预算月份：</label>
         </th>
         <td>
             <p class="form-control-static">@Model.BudgetMonth.ToString("yyyy年MM月")</p>
         </td>
     </tr>
 </table>
 <div class="datagrid" id="datagrid" data-url="@Html.Raw(Url.Action("../HrBudgetDetails/DetailsGrid",new { BudgetId = Model.Id}))">
 </div>