@model GoOutRegistration
@{
    var title = "查看外出登记";
}

<div class="container-edit-zeniths">
    <div class="panel panel-default panel-zeniths">
        <div class="panel-heading">
            <span class="prefix"> <strong>@Model.Id</strong></span>
            @title
        </div>
        <div class="panel-body">
            @Html.AntiForgeryToken()
            <input type="hidden" name="@nameof(Model.Id)" value="@Model.Id" />
            <table class="table table-form table-dialog-form">
                <tr>
                    <th class='w-100px'><label class="control-label">姓名</label></th>
                    <td>
                        <p class="form-control-static">@Model.EmployeeName</p>
                        </td>
                    </tr>
                    <tr>
                        <th><label class="control-label">外出事由</label></th>
                        <td>
                            <p class="form-control-static">@Model.GoOutReason</p>
                        </td>
                    </tr>
                    <tr>
                        <th><label class="control-label">外出时间</label></th>
                        <td>
                            <p class="form-control-static">@DateTimeHelper.FormatDateHasSecond(Model.GoOutTime)</p>
                        </td>
                    </tr>
                    <tr>
                        <th><label class="control-label">预计返回时间</label></th>
                        <td>
                            <p class="form-control-static">@DateTimeHelper.FormatDateHasSecond(Model.PlanBackTime)</p>
                        </td>
                    </tr>
                    <tr>
                        <th><label class="control-label">申请时间</label></th>
                        <td>
                            <p class="form-control-static">@DateTimeHelper.FormatDateHasSecond(Model.ApplyTime)</p>
                        </td>
                    </tr>
                    <tr>
                        <th><label class="control-label">实际返回时间</label></th>
                        <td>
                            <p class="form-control-static">@DateTimeHelper.FormatDateHasSecond(Model.RealBackTime)</p>
                        </td>
                    </tr>
                    <tr>
                        <th><label class="control-label">备注</label></th>
                        <td>
                            <p class="form-control-static">@Model.Note</p>
                        </td>
                    </tr>
                </table>
            </div>
            <div class="panel-footer text-center">
                <div class="buttons">
                    <a class="btn btn-default btnClose" onclick="zeniths.util.closeFrameDialog(window);">
                        <i class="fa fa-sign-in"></i> 关闭
                    </a>
                </div>
            </div>
        </div>
    </div>
