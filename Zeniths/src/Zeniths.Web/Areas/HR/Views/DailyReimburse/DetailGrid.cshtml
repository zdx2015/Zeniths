@model List<DailyReimburseDetails>
@{
    Layout = null;
}
<table class="table table-zeniths table-striped table-bordered table-hover">
    <thead>
    <tr>
        <th class="w-100px">项目名称</th>
        <th class="w-100px" data-order="@nameof(DailyReimburseDetails.CategoryName)">费用类别名称</th>
        <th class="w-100px" data-order="@nameof(DailyReimburseDetails.Amount)">费用金额</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var record in Model)
    {
        <tr>
            <td>@record.ItemName</td>
            <td>@record.CategoryName</td>
            <td>@record.Amount</td>
        </tr>
    }
    </tbody>
</table>
    
     



